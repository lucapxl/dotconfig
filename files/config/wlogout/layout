{
    "label" : "logout",
    "action" : "loginctl terminate-user $USER",
    "text" : "Logout",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "if command -v systemctl; then systemctl poweroff; else loginctl poweroff; fi",
    "text" : "Shutdown",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "if command -v systemctl; then systemctl suspend; else loginctl suspend; fi",
    "text" : "Suspend",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "if command -v systemctl; then systemctl reboot; else loginctl reboot; fi",
    "text" : "Reboot",
    "keybind" : "r",
}
