# to get outputs: swaymsg -t get_outputs

# Home Screen
profile Home {
	output eDP-1 disable
	output "Lenovo Group Limited R45w-30 UPP03XBK" position 0,0
	exec $HOME/.config/scripts/kanshi.sh notify "Home Screen"
}

# Office Docking Station
profile VPDora {
	output "Najing CEC Panda FPD Technology CO. ltd 0x0035 Unknown" position 0,0
	exec $HOME/.config/scripts/kanshi.sh notify "VPDora"
}

# Screen in qemu
profile Virtual {
	output Virtual-1 mode 1440x900 position 0,0
	exec $HOME/.config/scripts/kanshi.sh notify "Virtual monitor"
}


# ThinkPad Monitor
profile ThinkPad {
	output "Chimei Innolux Corporation 0x14D4 0x00000000" enable
	exec $HOME/.config/scripts/kanshi.sh notify "ThinkPad E14"
}

# ThinkPad Monitor
profile HPe {
	output "Chimei Innolux Corporation 0x13A9 Unknown" enable
	exec $HOME/.config/scripts/kanshi.sh notify "HPe g0tth4rd"
}