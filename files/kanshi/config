# to get outputs: swaymsg -t get_outputs

# Screen in Switzerland
profile HomeCH {
	output eDP-1 enable position 0,200
	output "Unknown HP 27m 1CR937052Z" mode 1920x1080 position 1920,0
}

# Screen in qemu
profile Virtual {
	output Virtual-1 mode 1440x900 position 0,0
}

# MacBook Monitor
profile MacBook {
	output "Apple Computer Inc Color LCD 0x00000000" scale 1.5
}
